    <nav class='header__nav-items'>
      <% if @hasBusinesses: %>
        <% for business in @businesses: %>
          <a id="business_<%= business.id %>" class='header__nav-item' href='/businesses/<%= business.id %>/standard_folders'>
            <%= business.name %>
          </a>
        <% end %>
      <% end %>
      <a id='clients_tab' class='header__nav-item header__nav-item--active' href='/clients'>
        Clients
      </a>
      <a id='workflows_tab' class='header__nav-item' href='/workflows'>
        Workflows
      </a>
      <% if @isSupport: %>
        <a id='statistics_tab' class='header__nav-item' href='/statistics'>
          Statistics
        </a>
        <a id='latest_documents_tab' class='header__nav-item' href='/latest_documents'>
          Latest Documents
        </a>
      <% end %>
    </nav>
  </div>
  <div>
    <div class='header__item-wrapper'>
      <a href='/messaging' >
        <span id="messaging-tab" class='header__notifications'>
          <i class='icon icon-chat-round'></i>
        </span>
      </a>
    </div>
    <div class='header__item-wrapper'>
      <a href='#'>
        <span class='header__notifications'>
          <i class='icon icon-cloud'></i>
        </span>
      </a>
    </div>
    <div class='header__item-wrapper'>
      <a href='/review_documents'>
        <span class='header__notifications' id='review-documents-tab'>
          <i class='icon icon-doc-box'></i>
        </span>
      </a>
    </div>
    <div class='header__item-wrapper' id='notifications-dropdown'>
      <span class='header__notifications' id='notification-icon'>
        <i class='icon icon-bell'></i>
      </span>
      <div class='header__dropdown-menu-wrapper' id='header-notifications-region'></div>
    </div>
    <div class='header__item-wrapper' id='user-dropdown'>
      <div class='header__userpic' id='user-icon'>
        <% if @avatarUrl: %>
          <img src="<%= @avatarUrl %>" class='header__userpic-img'>
        <% else: %>
          <i class='clients__client-status-icon icon icon-smile in-blue-400'></i>
        <% end %>
      </div>
      <div class='header__dropdown-menu-wrapper'>
        <ul class='header__dropdown-menu'>
          <li>
            <button class="header__dropdown-workspace-wrap change-workspace-js">
              <div class="header__dropdown-image-wrapper">
                <img src="<%= @avatarUrl %>" class='header__userpic-img'>
              </div>
              <div class="header__dropdown-workspace">
                <p class="header__dropdown-workspace-name">Personal</p>
                <p class="header__dropdown-company-name"><%= Docyt.currentAdvisor.get('full_name') %></p>
              </div>
            </button>
          </li>
          <% if @myBusiness: %>
            <% for item in @myBusiness: %>
              <li>
                <a class="header__dropdown-workspace-wrap header__dropdown-select-biz-js <%= @isSelectedBiz(item.id) %>" href="javascript:void(0)" biz-id="<%= item.id %>">
                  <div class="header__dropdown-image-wrapper">
                    <img src="<%= Docyt.currentAdvisor.getAmazonS3Url() + item.avatar.s3_object_key if item.avatar? %>" class='header__userpic-img'>
                  </div>
                  <div class="header__dropdown-workspace">
                    <p class="header__dropdown-workspace-name"><%= item.name %></p>
                    <p class="header__dropdown-company-name"><%= Docyt.currentAdvisor.get('full_name') %></p>
                  </div>
                </a>
              </li>
            <% end %>
          <% end %>
          <li>
            <a href='/billing' class='billing-link'>
              <icon class="icon icon-billing"></icon>
              Billing
            </a>
          </li>
          <li>
            <a href='/profile' class='profile-link'>
              <icon class="icon icon-settings"></icon>
              Settings
            </a>
          </li>
          <li>
            <button type="button" class='sign-out-js'>
              <icon class="icon icon-signout"></icon>
              Sign Out
            </button>
          </li>
        </ul>
      </div>
    </div>
  </div>
</header>
